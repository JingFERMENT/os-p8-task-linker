{% extends 'base.html.twig' %}

{% block title %}{{form.vars.value.name}}{% endblock %}

{% block body %}
<main>
    <header>
        <div id="current-user"></div>
        <div id="title-page">
            <h1>{{form.vars.value.name}}</h1>
        </div>
    </header>
    <div id="content">
       {{form_start(form)}}

			{{form_row(form.name)}}
			{{form_row(form.employees)}}
             <br>
			<button class="button button-submit">Modifier</button>

			{{form_end(form)}}
    </div>
</main>
{% endblock %}
